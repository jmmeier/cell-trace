		<dic class="row" style="background-color:white; opacity:0.7;"> 	
			<form id="addUntracedForm" role="form" method="get" action="/addCell" style="background-color:white; opacity:0.7; padding:10px">
				<h3>Add new untraced image</h3>
			    <div class="form-group">
			      <label for="untraced">URL:</label>
			      <input type="text" class="form-control" id="untraced" placeholder="URL to untraced image (.jpg or .png)" name="untraced">
			    </div>
			    
			    <input type="submit" id="submitBtn" class="btn btn-default" value="Add cell"></input>
			</form>
		</div>


			  <!--style="background-color:white; opacity: 0.4;"-->
		<h1 style="margin-left:20px"> Traces </h1>
		{{#each cells}}
			<div class="row" style="background-color:white; opacity:0.7;">
				<div class="col-md-1"></div>
				
				<div class="cell col-md-4">
	       			<h2> Untraced Cell </h2> <img src="{{untraced}}">
	       			<h6> <a href="{{untraced}}"> Link </a>
		    	</div>
		    	<div class="cell col-md-4">
	       			<h2> Traced Cell </h2> <img src="{{traced}}">
	       			<h6> <a href="{{traced}}" target="_blank"> Link </a>
		    	</div>
			</div>	
			<br><br>
    	{{/each}}

		
